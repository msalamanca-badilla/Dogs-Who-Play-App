<%- include('../partials/header') %>

<h1 class = 'myevents'>MY DOGS</h1>


<form action ='/profiles' method='POST'></form>
<table class = 'dogtable' width ='600px'>
  <thead>
    <tr class = 'doghead'>
      <th name ='Name of Dog' width = '100px'>Dog Name</th>
      <th name ='Location' width = '100px'>Location</th>
      <th name ='Age' width = '100px'>Age</th>
      <th name ='Breed'width = '100px'>Breed</th>
      <th></th>
    </tr>
  </thead>
  <% if (dog._id.equals(dog && dog._id)) { %>
  <tbody class ='dogbody'>
    <% dog.profile.forEach(function(p){%>
        <tr>
          <td><%= p.dogName %></td>
          <td><%= p.location %></td>
          <td><%= p.dogAge %></td>
          <td><%= p.breed %></td>
          <td><button class = 'detailsbutton'><a href = '/profiles/<%=p._id%>'>Profile Page</a></button></td>
        </tr>
    <%});%>
  </tbody>
  <%};%>
</table>
</form>

<button class = 'adddogbutton'><a href = '/profiles/adddog'>ADD DOG</a></button>

<%- include('../partials/footer') %>