<%- include('../partials/header') %>
<h1 class = 'myevents'>Upcoming Events</h1>
<form action ='/events' method='POST'></form>
<table class = 'eventtable' width ='800px'>
  <thead>
    <tr>
      <th name ='Event Name' width = '100px'>Event Name</th>
      <th name ='Event Location' width = '100px'>Event Location</th>
      <th name ='Add Description' width = '100px'>Description</th>
      <th name ='Date'width = '100px'>Date</th>
      <th name ='Host'width = '100px'>Host</th>
    </tr>
  </thead>
  <tbody>
    <% if (dog) { %>
      <tr>
        <td><%= events.eventName %></td>
        <td><%= events.eventLocation %></td>
        <td><%= events.addDescription %></td>
        <td><%= events.dateOfEvent %></td>
        <td><%= dog.name %></td>
      </tr>
  <%};%>
  </tbody>
</table>
</form>
<%- include('../partials/footer') %>