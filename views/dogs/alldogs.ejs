<%- include('../partials/header') %>
<h1 class = 'myevents'>All Dogs</h1>
<form class = 'profile' action ='/profiles' method='POST'>
<table class = 'eventtable' width ='800px'>
  <thead>
    <tr>
      <th name ='dogName' width = '100px'>Dog Name</th>
      <th name ='dogAge' width = '100px'>AGE</th>
      <th name ='location'width = '100px'>LOCATION</th>
      <th name ='breed'width = '100px'>BREED</th>
    </tr>
  </thead>
  <tbody>
    <% allProfiles.forEach(function(p){%>
      <tr>
        <td><%= p.dogName %></td>
        <td><%= p.dogAge %></td>
        <td><%= p.location %></td>
        <td><%= p.breed %></td>
    <% }) %>
</tr>
  
  </tbody>
</table>
</form>

<%- include('../partials/footer') %>