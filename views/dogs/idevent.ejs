<%- include('../partials/header') %>

<h1 class = 'eventname'><%= events.eventName %></h1>
<p class = 'eventdate'><em><%= events.dateOfEvent %></em></p>
    <div id = 'profilepic'></div>
<p class = 'location'><em>Location: <%= events.eventLocation %></em></p>
<p class = 'description'><%= events.addDescription %></p>


<button class = 'updateeventbutton'><a href = '/events/<%= events._id %>/updateevent'>UPDATE EVENT</a></button>
  

<% if (dog) { %>
<form class = 'deleteform' action="/events/<%= events._id %>?_method=DELETE" class="delete-form" method="POST">
    <button class = 'deletebutton' type="submit">DELETE EVENT</button>
</form>
<% } else { %>
    <div></div>
<% } %> 




<%- include('../partials/footer') %>